{
  "name": "spanai-api",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "workspaces": {
    "nohoist": [
      "@types/ioredis",
      "nest-redis",
      "@nestjs/**"
    ]
  },
  "scripts": {
    "build": "export NODE_ENV=production && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "mv:install": "mv node_modules/nest-redis ~/ && yarn && mv ~/nest-redis node_modules/",
    "pm2": "pm2 start pm2.yml",
    "publish": "scp -r dist liuchen@www.greatwebtech.cn:~/alaric/spansurvey/spanai-api/",
    "qinghua": "rsync -a -e 'ssh -p 22222' src root@101.6.9.6:/qinghua/spansurvey/spanai-api",
    "reload": "export NODE_ENV=production && nest build && pm2 reload span",
    "start": "export NODE_ENV=development && nest start --watch",
    "start:debug": "export NODE_ENV=development && nest start --debug --watch",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:prod": "export NODE_ENV=production && node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@alicloud/sms-sdk": "^1.1.6",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "gpt-tokens": "^1.1.2",
    "joi": "^17.6.3",
    "json2csv": "^6.0.0-alpha.2",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mongoose": "^6.6.5",
    "nest-redis": "https://github.com/hydemo/nestjs-redis.git#master",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.8.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "qiniu": "^7.8.0",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "28.1.8",
    "@types/md5": "^2.3.2",
    "@types/node": "^16.0.0",
    "@types/node-schedule": "^2.1.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
